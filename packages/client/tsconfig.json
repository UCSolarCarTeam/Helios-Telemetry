{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "checkJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/server/*": ["../server/src/*"],
      "@/shared/vehicleVelocity": ["../shared/src/vehicleVelocity"],
      "@/shared/types": ["../shared/src/types.ts"]
    }
  },
  "include": [
    ".eslintrc.cjs",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.cjs",
    "**/*.mjs"
  ],
  "exclude": ["node_modules", "tailwind.config.ts"]
}
