    "axios": "^1.7.2",
    "axios-retry": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "log4js": "^6.9.1",
    "module-alias": "^2.2.3",
    "mqtt": "^5.8.0",
    "socket.io": "^4.7.5",
    "sqlite": "^5.1.1",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
